@model IEnumerable<MVCDemo1.EmployeeData>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EmpName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Department)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HourlySal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bonusrate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeType.EmpType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EmpName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Department)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HourlySal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bonusrate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeType.EmpType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EmpID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EmpID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EmpID })
        </td>
    </tr>
}

</table>
